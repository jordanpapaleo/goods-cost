{
  "name": "goods-cost",
  "version": "1.0.0",
  "description": "A web seed project using webpack, react, and es6",
  "main": "./app/index.js",
  "scripts": {
    "lint": "standard ./**/*.js, ./**/*.jsx, *.js | standard-reporter --stylish",
    "test": "npm run lint && npm run unit-test",
    "unit-test": "mocha --compilers js:babel-core/register -R mocha-better-spec-reporter --check-leaks ./app/**/*.test.js",
    "build-dist": "rm -rf ./dist && webpack --config webpack.config.js --bail -p",
    "dist": "npm test && npm run build-dist",
    "dev": "webpack-dev-server --inline --hot"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "3.3.6",
    "gsap": "1.18.2",
    "history": "1.17.0",
    "parse": "1.6.14",
    "react": "0.14.5",
    "react-dom": "0.14.5",
    "react-redux": "4.0.6",
    "react-router": "^1.0.3",
    "redux": "3.0.5",
    "redux-form": "4.1.5",
    "redux-router": "^0.1.0",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "babel-core": "6.3.26",
    "babel-loader": "6.2.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "css-loader": "0.23.1",
    "debug": "2.2.0",
    "deep-freeze": "0.0.1",
    "expect": "1.13.4",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "1.7.0",
    "json-loader": "0.5.4",
    "less": "2.5.3",
    "less-loader": "2.2.2",
    "mocha": "2.3.4",
    "mocha-better-spec-reporter": "3.0.1",
    "redux-devtools": "3.0.1",
    "redux-logger": "2.4.0",
    "standard": "5.4.1",
    "standard-reporter": "1.0.5",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.9",
    "webpack-dev-server": "1.14.0"
  }
}
